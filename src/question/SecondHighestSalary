-- LeetCode
-- 2018.07.09.下午3:19
--
-- 176. Second Highest Salary
-- https://leetcode.com/problems/second-highest-salary/description/
--
-- @author : rick
--

SELECT Salary AS SecondHighestSalary FROM Employee UNION ALL (SELECT NULL AS SecondHighestSalary) ORDER BY SecondHighestSalary DESC LIMIT 1 OFFSET 1;
